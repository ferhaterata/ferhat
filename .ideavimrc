" https://github.com/JetBrains/ideavim
" ====================================
" Use Vim settings, rather than Vi settings (much better!).
" This must be first, because it changes other options as a side effect.
set nocompatible

" search for actions: :actionlist <patter>

set number                        " line numbers
set relativenumber
set hlsearch                      " highlight search occurrences
set ignorecase                    " ignore case in search patterns
set smartcase                     " no ignore case when pattern is uppercase
set incsearch                     " show search results while typing
set wrapscan                      " searches wrap around the end of the file
set gdefault                      " substitute all occurrences in line per default
set history=10000                 " keep x lines of command line history
set scrolloff=3 sidescrolloff=10  " keep some lines before and after the cursor visible
set clipboard=unnamedplus,unnamed,ideaput " integrate with system clipboard

set visualbell
set noerrorbells

set ttimeout
set ttimeoutlen=400

"nnoremap <SPACE> <Nop>
"let mapleader="\\"
let maplocalleader=","		
set lookupkeys+=<C-K>
" https://github.com/JetBrains/ideavim/blob/master/doc/emulated-plugins.md
" ========================================================================
set ideamarks                     "https://github.com/JetBrains/ideavim/blob/master/doc/set-commands.md" 
set ideajoin                      "https://github.com/JetBrains/ideavim/wiki/%60ideajoin%60-examples"
" Commands: <A-n>, <A-x>, <A-p>, g<A-n>
set multiple-cursors              "https://github.com/terryma/vim-multiple-cursors"
" Commands: gcc, gc + motion, v_gc
set commentary                    "https://github.com/tpope/vim-commentary 
" Commands: ys, cs, ds, S
set surround                      "https://github.com/tpope/vim-surround                      
" Commands: gr, grr
set ReplaceWithRegister           "https://github.com/vim-scripts/ReplaceWithRegister"
" Additional text objects: aa, ia
set argtextobj                    "https://www.vim.org/scripts/script.php?script_id=2699"
let g:argtextobj_pairs="(:),{:},<:>"
" Additional text objects: ae, ie
set textobj-entire                "https://github.com/kana/vim-textobj-entire"
" Commands: https://github.com/AlexPl292/IdeaVim-EasyMotion#supported-commands 
set easymotion                    "https://github.com/AlexPl292/IdeaVim-EasyMotion"

" https://github.com/JetBrains/ideavim/wiki/Select-mode
set idearefactormode=visual
"set idearefactormode=keep

" `s{char}{label}`
" nmap s <Plug>(easymotion-f)
" `s{char}{char}{label}`
" Need one more keystroke, but on average, it may be more comfortable.
nmap s <Plug>(easymotion-f2)
nmap <leader>j <Plug>(easymotion-j)
nmap <leader>k <Plug>(easymotion-k)

"  Default Mapping |  <Plug> command       |
"  -----------------------------------------------------------------
"   <ll>f{char}    |  <Plug>(easymotion-f) |  mapped to fn
"   <ll>F{char}    |  <Plug>(easymotion-F) |  mapped to Fn
"   <ll>t{char}    |  <Plug>(easymotion-t) |  mapped to tn
"   <ll>T{char}    |  <Plug>(easymotion-T) |  mapped to Tn
"
"   <ll>w          |  <Plug>(easymotion-w) |
"   <ll>W          |  <Plug>(easymotion-W) |
"   <ll>b          |  <Plug>(easymotion-b) |
"   <ll>B          |  <Plug>(easymotion-B) |
"   <ll>e          |  <Plug>(easymotion-e) |
"   <ll>E          |  <Plug>(easymotion-E) |
"   <ll>ge         |  <Plug>(easymotion-ge |
"   <ll>gE         |  <Plug>(easymotion-gE |
"   <ll>j          |  <Plug>(easymotion-j) |
"   <ll>k          |  <Plug>(easymotion-k) |
"   <ll>n          |  <Plug>(easymotion-n) |
"   <ll>N          |  <Plug>(easymotion-N) |
"   <ll>s          |  <Plug>(easymotion-s) |  mapped to sn
" ========================================================================

" smash escape
inoremap jk <Esc>

"Allow normal up/down movement across wrapped lines
noremap j gj
noremap k gk

nnoremap L $
nnoremap H ^

" Bash like
inoremap <C-a> <Home>
inoremap <C-e> <End>
inoremap <C-d> <Delete>


" built in search looks better
nnoremap g/ :action Find<cr>
" but preserve ideavim search
nnoremap / /
" clear the search buffer when hitting return
nnoremap <cr> :nohlsearch<cr>

" built-in navigation to navigated items works better
nnoremap <c-o> :action Back<cr>
nnoremap <c-i> :action Forward<cr>
" but preserve ideavim defaults
nnoremap g<c-o> <c-o>
nnoremap g<c-i> <c-i>

" <leader>vv brings up .vimrc
" <leader>vr reloads it and makes all changes active (file has to be saved first)
noremap <leader>oe :e /home/ferhat/.ideavimrc<CR>
noremap <leader>R :source /home/ferhat/.ideavimrc<CR>:echo 'vimrc reloaded'<CR>

""""""""""""""""""""""""""""""""""""""""""""""""""""""
" Fix some vim annoyances

" make Y behave like D and C, yanking till end of line
map Y y$

" don't overwrite register when pasting over selection
vnoremap p pgvy

" don't lose selection when indenting
vnoremap < <gv
vnoremap > >gv
vnoremap = =gv


